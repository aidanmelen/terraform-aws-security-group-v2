{
    "build": { "dockerfile": "Dockerfile" },
    "extensions": [
        "hashicorp.terraform",
        "golang.Go",
        "streetsidesoftware.code-spell-checker"
    ],
    "mounts": [
        "source=${localEnv:HOME}/.aws,target=/root/.aws,type=bind",
        "source=${localEnv:HOME}/.cache/pre-commit,target=/root/.cache/pre-commit,type=bind",
    ]
}
